{
  "expo": {
    "name": "LIFTEZ",
    "slug": "frontend_react",
    "entryPoint": "./app/index.tsx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_liftez.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo_liftez.png",
      "resizeMode": "contain",
      "backgroundColor": "#2a313b"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2a313b"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "expo": {
      "web": {
        "bundler": "metro",
        "output": "static",
        "favicon": "./assets/images/favicon.png"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
